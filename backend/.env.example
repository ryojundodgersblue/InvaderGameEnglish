# Node環境
NODE_ENV=development

# サーバーポート
PORT=4000

# Google Sheets設定
SHEET_ID=your_spreadsheet_id_here
GOOGLE_KEYFILE=./credentials.json

# JWT設定
JWT_SECRET=your_secret_key_change_in_production_use_long_random_string

# Redis設定（パフォーマンス最適化用）
# ローカル開発環境では localhost、本番環境では適切なRedisサーバーを指定
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=your_redis_password_if_needed

# フロントエンドURL（CORS設定用）
FRONTEND_URL=http://localhost:5173
